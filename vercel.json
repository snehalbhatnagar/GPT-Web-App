{
    "version": 2,

    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/server.js"
      },
      {
        "src": "/index.js",
        "dest": "/public/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/views/$1"
      }
    ]
  }
  